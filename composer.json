{
  "name": "inaling/phulp-shell",
  "description": "The Shell addon for phulp",
  "keywords": ["phulp", "phulpplugin"],
  "homepage": "https://github.com/inaling/phulp-shell",
  "type": "library",
  "license": "MIT",
  "version": "0.3.0",
  "authors": [{
    "name": "Kazuhiro Inari",
    "email": "inarinlab@gmail.com"
  }],
  "require": {
    "php": ">=5.6",
    "reisraff/phulp": "~1.12"
  },
  "require-dev": {
    "peridot-php/peridot": "~1.19",
    "peridot-php/leo": "~1.6"
  },
  "autoload" : {
    "psr-4" : {
      "Phulp\\Plugin\\" : "src"
    }
  },
  "bin": [
    "bin/phulpsh"
  ],
  "scripts": {
    "test": "./vendor/bin/peridot test --force-colors"
  }
}
